{
    "version": 2,
    "builds": [
      {
        "src": "wims/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "wims/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "wims.settings"
    },
    "build": {
      "commands": [
        "python3 manage.py collectstatic --noinput"
      ]
    }
  }